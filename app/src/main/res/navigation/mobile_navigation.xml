<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.ios.nequixofficialv2.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home">
        <argument
            android:name="user_phone"
            app:argType="string" />
    </fragment>

    <fragment
        android:id="@+id/navigation_movements"
        android:name="com.ios.nequixofficialv2.ui.movements.MovementsFragment"
        android:label="@string/title_movements"
        tools:layout="@layout/fragment_movements">
        <argument
            android:name="user_phone"
            app:argType="string" />
    </fragment>

    <fragment
        android:id="@+id/navigation_services"
        android:name="com.ios.nequixofficialv2.ui.services.ServicesFragment"
        android:label="@string/title_services"
        tools:layout="@layout/fragment_services">
        <argument
            android:name="user_phone"
            app:argType="string" />
    </fragment>

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.ios.nequixofficialv2.ui.profile.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_profile">
        <argument
            android:name="user_phone"
            app:argType="string" />
    </fragment>

</navigation>
